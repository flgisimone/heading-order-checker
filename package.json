    "dev": "tsc -w",
    "test": "echo \"No tests yet!\""
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/react": "^19.0.10",
    "rollup": "^4.35.0",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "vue": "^3.0.0 || ^3.5.0"
  },
  "dependencies": {
    "tslib": "^2.8.1"
